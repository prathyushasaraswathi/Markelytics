<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@android:color/white"    android:orientation="vertical"    android:weightSum="9" >    <LinearLayout        android:id="@+id/linear2"        android:layout_width="match_parent"        android:layout_height="0dip"        android:layout_gravity="center"        android:layout_weight=".8"        android:background="@drawable/bg_two"        android:orientation="horizontal"        android:weightSum="4" >        <TextView            android:id="@+id/txt_t1"            android:layout_width="0dip"            android:layout_height="wrap_content"            android:layout_weight=".5" />        <TextView            android:id="@+id/txt_viewDetails"            android:layout_width="0dip"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_weight="3"            android:gravity="center"            android:text="View Profile Details"            android:textColor="@color/white"            android:textSize="20sp"            android:textStyle="bold" />        <TextView            android:id="@+id/txt_t2"            android:layout_width="0dip"            android:layout_height="wrap_content"            android:layout_weight=".5" />    </LinearLayout>    <ScrollView        android:id="@+id/scrollview"        android:layout_width="match_parent"        android:layout_height="0dip"        android:layout_weight="7"        android:fadeScrollbars="false"        >        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="10dp"            android:layout_marginRight="10dp"            android:orientation="vertical" >            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal"                android:weightSum="2" >                <TextView                    android:id="@+id/txt_email"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".8"                    android:text="Email Address"                    android:textColor="@color/black"                    android:textSize="15sp" />                <TextView                    android:id="@+id/txt_dot1"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".08"                    android:text=":" />                <EditText                    android:id="@+id/edt_email"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_weight="1.1"                    android:enabled="false"                    android:inputType="textEmailAddress"                    android:padding="5dp"                    android:textSize="15sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal"                android:weightSum="2" >                <TextView                    android:id="@+id/txt_firstName"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".8"                    android:text="First Name"                    android:textColor="@color/black"                    android:textSize="15sp" />                <TextView                    android:id="@+id/txt_dot2"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".08"                    android:text=":" />                <EditText                    android:id="@+id/edt_firstName"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_weight="1.1"                    android:enabled="false"                    android:inputType="textPersonName"                    android:padding="5dp"                    android:textSize="15sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal"                android:weightSum="2" >                <TextView                    android:id="@+id/txt_lastName"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".8"                    android:text="Last Name"                    android:textColor="@color/black"                    android:textSize="15sp" />                <TextView                    android:id="@+id/txt_dot3"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".08"                    android:text=":" />                <EditText                    android:id="@+id/edt_lastName"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_weight="1.1"                    android:enabled="false"                    android:inputType="textPersonName"                    android:padding="5dp"                    android:textSize="15sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal"                android:weightSum="2" >                <TextView                    android:id="@+id/txt_gender"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".8"                    android:text="Gender"                    android:textColor="@color/black"                    android:textSize="15sp" />                <TextView                    android:id="@+id/txt_dot4"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".08"                    android:text=":" />                <RelativeLayout                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_weight="1.1" >                    <EditText                        android:id="@+id/edt_gender"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_alignRight="@+id/rdg_gender"                        android:enabled="false"                        android:inputType="text"                        android:padding="5dp"                        android:textSize="15sp" />                    <RadioGroup                        android:id="@+id/rdg_gender"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal"                        android:visibility="gone" >                        <RadioButton                            android:id="@+id/gender_male"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="Male" />                        <RadioButton                            android:id="@+id/gender_female"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="Female" />                    </RadioGroup>                </RelativeLayout>            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal"                android:weightSum="2" >                <TextView                    android:id="@+id/txt_birthYear"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".8"                    android:text="Year Of Birth"                    android:textColor="@color/black"                    android:textSize="15sp" />                <TextView                    android:id="@+id/txt_dot5"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".08"                    android:text=":" />                <EditText                    android:id="@+id/edt_birthYear"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_weight="1.1"                    android:enabled="false"                    android:maxLength="4"                    android:padding="5dp"                    android:textSize="15sp"                    android:digits="0123456789"/>            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal"                android:weightSum="2" >                <TextView                    android:id="@+id/txt_streetAddress"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".8"                    android:gravity="left|top"                    android:text="Street Address"                    android:textColor="@color/black"                    android:textSize="15sp" />                <TextView                    android:id="@+id/txt_dot6"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".08"                    android:gravity="left|top"                    android:text=":" />                <EditText                    android:id="@+id/edt_streetAddress"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_weight="1.1"                    android:ems="10"                    android:enabled="false"                    android:gravity="start|top"                    android:inputType="textMultiLine"                    android:padding="5dp"                    android:text=""                    android:textSize="15sp" >                    <requestFocus />                </EditText>            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal"                android:weightSum="2" >                <TextView                    android:id="@+id/txt_zipCode"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".8"                    android:text="Post/Zip code"                    android:textColor="@color/black"                    android:textSize="15sp" />                <TextView                    android:id="@+id/txt_dot7"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".08"                    android:text=":" />                <EditText                    android:id="@+id/edt_zipCode"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_weight="1.1"                    android:enabled="false"                    android:padding="5dp"                    android:textSize="15sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal"                android:weightSum="2" >                <TextView                    android:id="@+id/txt_phone"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight="0.8"                    android:text="Phone Number"                    android:textColor="@color/black"                    android:textSize="15sp" />                <TextView                    android:id="@+id/txt_dot8"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_weight=".08"                    android:text=":" />                <EditText                    android:id="@+id/edt_phone"                    android:layout_width="0dip"                    android:layout_height="wrap_content"                    android:layout_weight="1.1"                    android:enabled="false"                    android:padding="5dp"                    android:textSize="15sp" />            </LinearLayout>            <!--<LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="20dp"                android:orientation="vertical" >                <TextView                    android:id="@+id/txt_paymentMethod"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:text="Payment Method :"                    android:textColor="@color/black"                    android:textSize="15sp" />                <RelativeLayout                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <EditText                        android:id="@+id/edt_paymentMethod"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="5dp"                        android:enabled="false"                        android:inputType="text"                        android:padding="5dp"                        android:textSize="15sp" />                    <Spinner                        android:id="@+id/spn_paymentMethod"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:enabled="false"                        android:background="@drawable/spinner_big_gray"                        android:visibility="gone" />                </RelativeLayout>            </LinearLayout>-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="20dp"                android:orientation="vertical" >                <TextView                    android:id="@+id/txt_surveyFreq"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:text="Ideal Survey Frequency :"                    android:textColor="@color/black"                    android:textSize="15sp" />                <RelativeLayout                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <EditText                        android:id="@+id/edt_surveyFreq"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="5dp"                        android:enabled="false"                        android:inputType="text"                        android:padding="5dp"                        android:textSize="15sp" />                    <Spinner                        android:id="@+id/spn_surveyFreq"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:enabled="false"                        android:background="@drawable/spinner_big_gray"                        android:visibility="gone" />                </RelativeLayout>            </LinearLayout>        </LinearLayout>    </ScrollView>    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="0dip"        android:layout_marginTop="5dp"        android:layout_weight="1"        android:orientation="horizontal" >        <com.andexert.library.RippleView            xmlns:ripple="http://schemas.android.com/apk/res-auto"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            ripple:rv_type="rectangle"            >        <Button            android:id="@+id/btn_edit"            android:layout_width="80dip"            android:layout_height="40dip"            android:layout_alignParentLeft="true"            android:background="@drawable/style_button"            android:text="EDIT"            android:textColor="@color/white"            android:elevation="15dp"            android:stateListAnimator="@null"            android:layout_marginLeft="50dp"/>        </com.andexert.library.RippleView>        <RelativeLayout            android:id="@+id/relativeLogout"            android:layout_width="80dip"            android:layout_height="40dip"            android:elevation="15dp"            android:stateListAnimator="@null"            android:background="@drawable/style_button"            android:layout_alignParentRight="true"            android:layout_marginRight="50dp"            >            <ImageView                android:layout_width="35dip"                android:layout_height="25dip"                android:src="@drawable/menu_logout"                android:layout_centerInParent="true"                />        </RelativeLayout>        <!-- <Button            android:id="@+id/btn_redeemRewards"            android:layout_width="170dip"            android:layout_height="40dip"            android:layout_alignParentRight="true"            android:layout_marginRight="25dip"            android:background="@drawable/style_button"            android:text="Redeem Rewards"            android:textColor="@color/white" /> -->        <Button            android:id="@+id/btn_save"            android:layout_width="80dip"            android:layout_height="40dip"            android:layout_centerInParent="true"            android:background="@drawable/style_button"            android:text="SAVE"            android:textColor="@color/white"            android:visibility="invisible"            android:elevation="15dp"            android:stateListAnimator="@null"/>    </RelativeLayout></LinearLayout>